{
  "name": "packt-vuex-condensed",
  "version": "1.0.0",
  "description": "Packt Vuex condensed",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "prestart": "npm install",
    "test": "karma start",
    "start": "npm run chapter-5",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "lint": "eslint chapter-3 chapter-5 test chapter-3/**/*.vue chapter-5/**/*.vue test/**/*.vue",
    "chapter-3": "webpack-dev-server --config chapter-3.webpack.config.js --mode development --open",
    "chapter-5": "webpack-dev-server --mode development --open",
    "chapter-2": "http-server ./chapter-2/ -c 1  -o"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PacktPublishing/-Vuex-Condensed.git"
  },
  "keywords": [
    "Vuex",
    "Vue",
    "Flux",
    "Packt",
    "JavaScript"
  ],
  "author": "Andrea Koutifaris",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PacktPublishing/-Vuex-Condensed/issues"
  },
  "homepage": "https://github.com/PacktPublishing/-Vuex-Condensed#readme",
  "dependencies": {
    "normalize.css": "^7.0.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.4.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jasmine": "^2.9.1",
    "eslint-plugin-vue": "^4.2.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^3.0.6",
    "http-server": "^0.11.1",
    "jasmine-core": "^2.9.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "karma-webpack": "^2.0.9",
    "vue-loader": "^14.2.1",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  }
